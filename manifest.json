{
  "manifest_version": 2,
  "name": "Emojeezer - Dev",
  "version": "2.2.5",
  "description": "A simple extension to type emojis using the :emoji: syntax.",
  "browser_specific_settings": {
    "gecko": {
      "id": "emoji@Choucroute-melba.github.io"
    }
  },
  "icons": {
    "48": "./assets/ninja.png",
    "72": "./assets/ninja-2.png",
    "256": "./assets/ninja-3.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "build.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}