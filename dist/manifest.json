{
  "manifest_version": 2,
  "name": "Emojeezer",
  "version": "2.2.1",
  "description": "A simple extension to type emojis using the :emoji: syntax.",
  "browser_specific_settings": {
    "gecko": {
      "id": "emoji@Choucroute-melba.github.io"
    }
  },
  "icons": {
    "48": "assets/icon@48px.png",
    "72": "assets/icon@72px.png",
    "256": "assets/icon@256px.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "build.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}